n class=3D"dt">limits =3D</span> <span class=3D"kw">c</span>(<span class=3D=
"st">"Class"</span>, <span class=3D"st">"Sex"</span>, <span class=3D"st">"S=
urvived"</span>),
                   <span class=3D"dt">expand =3D</span> <span class=3D"kw">=
c</span>(.<span class=3D"dv">1</span>, .<span class=3D"dv">1</span>)) <span=
 class=3D"op">+</span>
<span class=3D"st">  </span><span class=3D"kw">scale_fill_viridis_d</span>(=
) <span class=3D"op">+</span>
<span class=3D"st">  </span><span class=3D"kw">labs</span>(<span class=3D"d=
t">title =3D</span> <span class=3D"st">"Titanic data"</span>,
