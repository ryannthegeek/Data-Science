<div class=3D"sourceCode"><pre class=3D"sourceCode r"><code class=3D"source=
Code r"><span class=3D"kw">library</span>(PoEdata)
<span class=3D"kw">data</span>(<span class=3D"st">"food"</span>)
alpha &lt;-<span class=3D"st"> </span><span class=3D"fl">0.05</span>
x &lt;-<span class=3D"st"> </span><span class=3D"dv">20</span> <span class=
=3D"co"># income is in 100s, remember?</span>
m1 &lt;-<span class=3D"st"> </span><span class=3D"kw">lm</span>(food_exp~in=
come, <span class=3D"dt">data=3D</span>food)
tcr &lt;-<span class=3D"st"> </span><span class=3D"kw">qt</span>(<span clas=
s=3D"dv">1</span>-alpha/<span class=3D"dv">2</span>, df) <span class=3D"co"=
># rejection region right of tcr.</span>
df &lt;-<span class=3D"st"> </span><span class=3D"kw">df.residual</span>(m1=
)
b1 &lt;-<span class=3D"st"> </span>m1$coef[<span class=3D"dv">1</span>]
b2 &lt;-<span class=3D"st"> </span>m1$coef[<span class=3D"dv">2</span>]
varb1 &lt;-<span class=3D"st"> </span><span class=3D"kw">vcov</span>(m1)[<s=
pan class=3D"dv">1</span>, <span class=3D"dv">1</span>]
varb2 &lt;-<span class=3D"st"> </span><span class=3D"kw">vcov</span>(m1)[<s=
pan class=3D"dv">2</span>, <span class=3D"dv">2</span>]
covb1b2 &lt;-<span class=3D"st"> </span><span class=3D"kw">vcov</span>(m1)[=
<span class=3D"dv">1</span>, <span class=3D"dv">2</span>]
L &lt;-<span class=3D"st"> </span>b1+b2*x  <span class=3D"co"># estimated L=
</span>
varL =3D<span class=3D"st"> </span>varb1 +<span class=3D"st"> </span>x^<spa=
n class=3D"dv">2</span> *<span class=3D"st"> </span>varb2 +<span class=3D"s=
t"> </span><span class=3D"dv">2</span>*x*covb1b2 <span class=3D"co"># var(L=
)</span>
seL &lt;-<span class=3D"st"> </span><span class=3D"kw">sqrt</span>(varL) <s=
pan class=3D"co"># standard error of L</span>
lowbL &lt;-<span class=3D"st"> </span>L-tcr*seL
upbL &lt;-<span class=3D"st"> </span>L+tcr*seL</code></pre></div>
